/*
	HTTP Put Request after deleting package in package manager

	Request URL:http://localhost:3000/api/v1/sandboxes/${sandboxID}/modules/${moduleID}

	sandboxID: ID of sandbox (id field in getSandbox)
	moduleID: short ID of package.json'

	Sending content of newly created package.json file
*/

{
  "module": {
    "code":
      "{\n  \"name\": \"new\",\n  \"version\": \"1.0.0\",\n  \"description\": \"\",\n  \"keywords\": [],\n  \"homepage\": \"https://codesandbox.io/s/new\",\n  \"main\": \"src/index.js\",\n  \"dependencies\": {\n    \"react\": \"16.2.0\",\n    \"react-dom\": \"16.2.0\",\n    \"react-scripts\": \"1.1.0\"\n  },\n  \"devDependencies\": {},\n  \"scripts\": {\n    \"start\": \"react-scripts start\",\n    \"build\": \"react-scripts build\",\n    \"test\": \"react-scripts test --env=jsdom\",\n    \"eject\": \"react-scripts eject\"\n  }\n}\n"
  }
}
